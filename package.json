{
  "name": "battleship-game-gui-react-js",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "prop-types": "^15.6.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-rangeslider": "^2.2.0",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.16.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.0",
    "enzyme-to-json": "^3.3.5",
    "node-parameter-handler": "^2.0.0",
    "node-sass": "^4.11.0",
    "react-scripts": "^2.1.8"
  },
  "scripts": {
    "start": "generate-parameters && react-scripts start",
    "build": "generate-parameters && react-scripts build",
    "test": "react-scripts test --verbose",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js(x)",
      "!src/index.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "extra": {
    "node_parameter_handler": [
      {
        "source": "src/parameters.json.dist",
        "output": "src/parameters.json",
        "envMap": {
          "api.host": "BASE_URL"
        }
      }
    ]
  }
}
